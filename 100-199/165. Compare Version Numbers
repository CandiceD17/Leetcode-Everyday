class Solution {
public:
    int toNum(string a){
      stringstream ss(a);
      int n;
      ss>>n;
      return n;
    }
    int compareVersion(string version1, string version2) {
      if(version1==version2)
          return 0;
      int i1 = 0;
      int i2 = 0;
      string v1 = "";
      string v2 = "";
      version1+=".";
      version2+=".";
      while(i1<version1.size() || i2<version2.size()){
        if(i1==version1.size()){
            if(version2[i2]!='0' && version2[i2]!='.')
                return -1;
            i2++;
        }
        else if(i2==version2.size()){
            if(version1[i1]!='0' && version1[i1]!='.')
                return 1;
            i1++;
        }
        
        else if(version1[i1]=='.' && version2[i2]=='.'){
          int a = toNum(v1);
          int b = toNum(v2);
          if(a>b)
            return 1;
          else if(a<b)
            return -1;
          v1="";
          v2="";
          i1++; i2++;
        }
        
        else if(version1[i1]=='.'){
          v2+=version2[i2];
          i2++;
        }
        else if(version2[i2]=='.'){
          v1+=version1[i1];
          i1++;
        }
        
        else{
          v1+=version1[i1];
          v2+=version2[i2];
          i1++; i2++;
        }
      }
      return 0;
    }
};

2019.08.23
